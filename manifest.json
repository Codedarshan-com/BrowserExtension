{
  "manifest_version": 3,
  "name": "Algoezy by CodeDarshan",
  "version": "1.0",
  "description": "An extension to master DSA in the right way.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["https://leetcode.com/*"],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["fe/dist/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
